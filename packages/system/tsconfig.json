{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": ".",
    "experimentalDecorators": true,
    
    
    "lib": [
      "es2022"
    ],
    "module": "commonjs",
    "target": "es2022",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
